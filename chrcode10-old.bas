10 'Character Code 20 OLD
20 CLS
30 C=0
40 LOCATE C%20+6,C/20+5
50 PRINT CHR$(C);
60 C=C+1
70 IF C<256 GOTO 40
80 X=0
90 IF X<10 GOTO 120
100 LOCATE X+6,2
110 PRINT X/10
120 LOCATE X+6,3
130 PRINT X%10
140 LOCATE X+6,4
150 PRINT CHR$(23);
160 X=X+1
170 IF X<20 GOTO 90
180 Y=0
190 LOCATE 2,Y/20+5
200 IF Y=0 PRINT " ";
210 IF Y<100 PRINT " ";
220 PRINT Y;CHR$(22); 
230 Y=Y+20
240 IF Y<256 GOTO 190
250 LOCATE 5,4
260 PRINT CHR$(24);
270 LOCATE 0,22
